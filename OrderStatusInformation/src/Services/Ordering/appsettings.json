{
    "Serilog": {
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Warning",
          "Microsoft.Hosting.Lifetime": "Information",
          "System": "Warning"
        }
      },
      "WriteTo": [
        {
          "Name": "MongoDB",
          "Args": {
            "databaseUrl": "",
            "collectionName": "logs",
            "cappedMaxSizeMb": "1024",
            "cappedMaxDocuments": "50000"
          }
        }
      ]
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
      "OrderingConnectionString": "",
      "RabbitMQConnectionString": ""
    }
  }
